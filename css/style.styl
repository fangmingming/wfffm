.pop
    width:1033px;
    height:566px;
    position: relative
    font-family: '\65b9\6b63\5170\4ead\9ed1'
    display:none
    &-footer
        background:url("../images/pop-footer.png") no-repeat center bottom
        width:1033px;
        height:141px;
        position: absolute
        left:0
        bottom:0
        z-index: 9
    .btn-close
        display:block
        width:79px;
        height:196px;
        background-position -561px -22px;
        font-size: 0;
        position: absolute
        right:53px;
        top:49px;
        z-index:9;
        animation:ths 3s infinite linear
        transform-origin:51px 29px
    &-slider
        width:822px;
        height: 100%
        position: relative
        margin:0 auto
        img
            display:block
            width:822px;
            height:461px;
            position: absolute
            left:0
            bottom:13px
        h2
            height:63px;
            width:600px;
            margin:0 auto
            font-size: 0;
            background:url("../images/pop-title.png") no-repeat
            &.n1
                background-position 0 0
            &.n2
                background-position 0 -63px
            &.n3
                background-position 0 -127px
            &.n4
                background-position 0 -190px
            &.n5
                background-position 0 -253px
            &.n6
                background-position 0 -317px
        h3
            font-size: 16px;
            color:#8b857a
            text-align center;
            font-weight: normal;
            margin-top: -4px;
            font-weight: bold;
        .swiper-container
            width:100%
            height:100%
        .swiper-slide
            position: relative
        .swiper-button-next, .swiper-button-prev
            width:64px;
            height:64px;
            background:url("../images/spr.png") no-repeat
            margin-top: 10px;
        .swiper-button-next
            background-position -73px -221px
            right: -77px
        .swiper-button-prev
            background-position 0 -221px
            left: -77px
        .swiper-pagination
            position: relative
            padding:10px
        .swiper-pagination-bullet
            width:18px;
            height:18px;
            margin:0 10px;
            opacity 1
            background:url("../images/spr.png") no-repeat -202px -221px
        .swiper-pagination-bullet-active
            background-position -156px -221px

