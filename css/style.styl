.thanksgiving
    width:1200px;
    height:493px;
    position: absolute
    left:238px;
    top:281px;
    span
        display:block
        width:87px;
        height:272px;
        position: absolute
        font-size: 0;
        animation:ths 5s infinite linear
        transform-origin: 43px top;
        cursor: pointer;
        background:url("../images/thanksgiving-icon.png") no-repeat
        &.n1
            width:139px;
            height:277px;
            background-position 0 -166px;
            left:0
            top:166px
            transform-origin:75px top;
        &.n2
            background-position -258px -106px;
            left:258px
            top:106px
            animation-delay:0.3s
        &.n3
            background-position -451px 0;
            left:451px
            top:0
            animation-delay:1.2s
        &.n4
            height:295px;
            background-position -630px -74px;
            left:630px
            top:74px
            animation-delay:0.5s
        &.n5
            background-position -830px -21px;
            left:830px
            top:21px
            animation-delay:1.5s
        &.n6
            background-position -1007px -201px;
            left:1007px
            top:201px
            animation-delay:0.4s
@keyframes ths{
    0%,100%{transform:rotate(0deg)}
    30%{transform:rotate(1deg)}
    60%{transform:rotate(-2deg)}
}
